
# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# BunnyNet Configuration
EXPO_PUBLIC_BUNNY_STREAM_LIBRARY_ID=your_bunny_stream_library_id_here
EXPO_PUBLIC_BUNNY_STREAM_API_KEY=your_bunny_stream_api_key_here
EXPO_PUBLIC_BUNNY_CDN_HOSTNAME=your_bunny_cdn_hostname_here

# AWS Rekognition Configuration (for content moderation)
# These are set as Supabase Edge Function secrets AND Trigger.dev environment variables
# AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
# AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
# AWS_REGION=ap-southeast-2

# Trigger.dev Configuration (for video moderation)
# TRIGGER_PROJECT_ID=proj_your_trigger_project_id_here
# Set in Trigger.dev Dashboard:
#   - AWS_ACCESS_KEY_ID
#   - AWS_SECRET_ACCESS_KEY
#   - AWS_REGION
#   - SUPABASE_URL
#   - SUPABASE_SERVICE_ROLE_KEY
#
# Set in Supabase Edge Function Secrets:
#   - TRIGGER_SECRET_KEY=your_trigger_dev_secret_key_here
#   - TRIGGER_API_URL=https://api.trigger.dev (optional)

# Google OAuth Configuration
# Get this from Google Cloud Console: https://console.cloud.google.com/
# Create OAuth 2.0 Client ID for Web application
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here
